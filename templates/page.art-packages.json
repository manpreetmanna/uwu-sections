          }
        },
        "item_TzXChj": {
          "type": "item",
          "settings": {
            "icon": "shop",
            "heading": "CUSTOM DASHBOARD & TECH STACK",
            "text": "<p>Track your sales. Watch your progress on your UwU Milestone awards. Check your payouts. View your product notes and reply to customers, AND MORE!</p>",
            "image": "shopify://shop_images/Untitled_design_14.gif"
          }
        },
        "item_tfcRnh": {
          "type": "item",
          "settings": {
